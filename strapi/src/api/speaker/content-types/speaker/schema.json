{
  "kind": "collectionType",
  "collectionName": "speakers",
  "info": {
    "singularName": "speaker",
    "pluralName": "speakers",
    "displayName": "Speakers"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "photo": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "title": {
      "type": "string"
    },
    "bio": {
      "type": "richtext"
    },
    "events": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::event.event",
      "inversedBy": "speakers"
    }
  }
}
