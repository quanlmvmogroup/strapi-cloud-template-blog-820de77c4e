{
  "kind": "collectionType",
  "collectionName": "event_registrations",
  "info": {
    "singularName": "event-registration",
    "pluralName": "event-registrations",
    "displayName": "Event Registration"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "full_name": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email",
      "required": true
    },
    "phone": {
      "type": "string",
      "required": true
    },
    "company": {
      "type": "string"
    },
    "note": {
      "type": "text"
    },
    "event": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::event.event",
      "inversedBy": "event_registrations"
    }
  }
}
